{
    "name": "es-electron-boilerplate",
    "version": "1.0.0",
    "description": "Electron application boilerplate",
    "main": "./dist/main.js",
    "scripts": {
        "watch": "webpack --watch --mode=development",
        "build": "webpack --mode=production",
        "start": "electron ./dist/main.js",
        "pack": "build --mac --x64",
        "lint": "eslint app/**/*.js app/**/*.jsx",
        "lint:fix": "eslint --fix app/**/*.js app/**/*.jsx"
    },
    "author": "Ekilsid",
    "license": "MIT",
    "dependencies": {
        "connected-react-router": "^6.3.2",
        "history": "^4.9.0",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-redux": "^6.0.1",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.1",
        "styled-components": "^4.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.3.3",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "copy-webpack-plugin": "^5.0.0",
        "electron": "^4.0.4",
        "electron-builder": "^20.38.5",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "prettier": "^1.16.4",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3",
        "webpack-merge": "^4.2.1",
        "webpack-node-externals": "^1.7.2"
    },
    "build": {
        "productName": "Es-boilerplate",
        "appId": "com.ekilsid.electron.boilerplate",
        "directories": {
            "output": "release"
        },
        "mac": {
            "target": "dmg"
        }
    }
}
